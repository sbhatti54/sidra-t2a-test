<script type="text/javascript">
  $(function(){
      // create a datepicker with default settings
      $("#date_of_birth").mobiscroll().date();
  });

    (function($,W,D)
    {
        var MSG = {};

        MSG.UTIL =
        {
            setupFormValidation: function()
            {
                //form validation rules
                $("#form").validate({
                    rules: {
                        date_of_birth:{
                           required:true,
                            date:true
                        }
                    },
                    messages: {
                        date_of_birth: {
                            required: "Birth Date is required",
                            date:  "Birth Date must be in format mm/dd/yyyy"
                        }
                    },
                    submitHandler: function(form) {
                        setTimeout(function(){
                             // _gaq.push(['_trackEvent', 'sweeps', 'submit','submit']);
                           },500);
                        form.submit();
                    }
                });
            }
        }

        //when the dom has loaded setup form validation rules
        $(D).ready(function($) {
            MSG.UTIL.setupFormValidation();
        });

    })(jQuery, window, document);
</script>